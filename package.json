{
  "name": "feedropolis",
  "version": "1.0.0",
  "description": "rss feed generator",
  "scripts": {
    "watch-inner": "rm -rf dist/* && NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.config.dev.js --watch",
    "serve": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve",
    "build": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build && NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.config.prod.js",
    "start": "node bin/server.js",
    "migrate": "node bin/migrate.js",
    "build-docker": "podman build . -t docker.io/stormworks/feedropolis --pull"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@stormking/emere": "^2.0.1",
    "axios": "^1.6.2",
    "cheerio": "1.0.0-rc.12",
    "co-body": "^6.1.0",
    "core-js": "^3.33.2",
    "debug": "^4.3.4",
    "dotenv": "^8.6.0",
    "feed": "^4.2.2",
    "fetch": "^1.1.0",
    "knex": "^0.21.21",
    "koa": "^2.14.2",
    "koa-better-router": "^2.1.1",
    "koa-session": "^6.4.0",
    "koa-static": "^5.0.0",
    "lru-cache": "^6.0.0",
    "nightmare": "^3.0.2",
    "pg": "^8.11.3",
    "purecss": "^2.2.0",
    "vue": "^2.7.15",
    "xmldom": "^0.4.0",
    "xpath.js": "^1.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "css-loader": "^3.6.0",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^4.5.2",
    "less": "^3.13.1",
    "less-loader": "^6.2.0",
    "style-loader": "^1.3.0",
    "vue-template-compiler": "^2.7.15",
    "webpack": "^4.47.0",
    "webpack-cli": "^3.3.12"
  }
}
